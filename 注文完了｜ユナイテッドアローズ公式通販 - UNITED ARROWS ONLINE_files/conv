(function() {
    if (typeof document.featurePolicy != 'undefined' && document.featurePolicy != null && typeof document.featurePolicy.allowsFeature == 'function' && document.featurePolicy.allowsFeature('attribution-reporting')) {
        if (document.readyState === 'loading') {
            document.addEventListener('DOMContentLoaded', function() {
                if (document.body === null) {
                    document.body = document.createElement('body');
                }
                var img = document.createElement('img');
                img.style.display = 'none';
                img.style.width = '1px';
                img.style.height = '1px';
                img.src = '';
                img.setAttribute('attributionsrc', 'https://adv9892.ps.ladsp.com/attribution_trigger?advertiser_id=9892&trid=4997488593690867878&site_url=https%3A%2F%2Fstore.united-arrows.co.jp%2Forder%2Freg%3Fop%3Dcomplete&asr=false&sg=6b9da790f65ef918616411b25d63beef9424621d');
                document.body.appendChild(img);
            });
        } else {
            if (document.body === null) {
                document.body = document.createElement('body');
            }
            var img = document.createElement('img');
            img.style.display = 'none';
            img.style.width = '1px';
            img.style.height = '1px';
            img.src = '';
            img.setAttribute('attributionsrc', 'https://adv9892.ps.ladsp.com/attribution_trigger?advertiser_id=9892&trid=4997488593690867878&site_url=https%3A%2F%2Fstore.united-arrows.co.jp%2Forder%2Freg%3Fop%3Dcomplete&asr=false&sg=6b9da790f65ef918616411b25d63beef9424621d');
            document.body.appendChild(img);
        }
    }
})();
